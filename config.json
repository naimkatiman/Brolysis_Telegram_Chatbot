{
  "version": 2,
  "/chart": {
    "intervals": ["5m", "15m", "1h", "4h", "1D", "1W"],
    "default": {
      "symbol": "CAPITALCOM:US100",
      "interval": "1D",
      "theme": "dark",
      "height": 800,
      "width": 1200,
      "studies": [
        {
          "name": "Volume",
          "forceOverlay": true,
          "override": {
            "Volume.color.0": "rgba(247,82,95,0.3)",
            "Volume.color.1": "rgba(34,171,148,0.3)"
          }
        },
        {
          "name": "Supertrend",
          "input": {
            "Factor": 3,
            "Period": 10
          },
          "override": {
            "Up Trend.color": "rgb(8,153,129)",
            "Down Trend.color": "rgb(242,54,69)"
          }
        },
        {
          "name": "Relative Strength Index",
          "input": {
            "length": 14
          },
          "override": {
            "Plot.color": "rgb(33,150,243)",
            "UpperLimit.value": 70,
            "LowerLimit.value": 30
          }
        }
      ]
    }
  },
  "/crypto": {
